{
  "manifest_version": 2,
  "name": "MaMaMaMa",
  "description": "A simple drawing interface built for outsider artist Marie Bodson",
  "version": "0.1.0",
  "icons": { "128": "icon128.png" },
  "permissions": [ "activeTab" ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "MaMaMaMa"
  }
}